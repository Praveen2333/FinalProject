@model IEnumerable<LMS.Web.Models.Result>

@{
    ViewData["Title"] = "Index";
}

<h1 style="color:blue">Results</h1>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ExamName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Marks)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ExamName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Marks)
            </td>
            <td>
               
                <a asp-action="Details" asp-route-id="@item.ResultId">Details</a> |
               
            </td>
        </tr>
}
    </tbody>
</table>
