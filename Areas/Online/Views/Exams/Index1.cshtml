@model IEnumerable<LMS.Web.Models.Exam>

@{
    ViewData["Title"] = "Index";
}

<h1 style="color:blue">List of Exams</h1>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ExamName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ExamDescription)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ExamName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ExamDescription)
            </td>
            <td>
                
                <a asp-action="Details" asp-route-id="@item.ExamId">Details</a> |
               
            </td>
        </tr>
}
    </tbody>
</table>
<button
   asp-area="Online" class="btn btn-primary" asp-controller="QuestionAnswers" asp-action="Index1">Html Exam</button>